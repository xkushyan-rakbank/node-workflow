{
	"request": {
		"method": "POST",
		"url": "/api/customer-onboarding/v1/banks/RAK/otp",
		"bodyPatterns": [
			{
				"matchesJsonPath": "$[?(@.email == 'no_otp@email.com')]",
				"matchesJsonPath": "$[?(@.mobileNo == '9876543210')]"
			}
		]
	},
	"response": {
		"status": 400,
        "headers": {
            "Content-Type": "application/json",
            "Access-Control-Allow-Origin" : "*",
            "Access-Control-Allow-Methods" : "*",
            "Access-Control-Allow-Headers": "Accept, Content-Type, Content-Encoding, Server, Transfer-Encoding, X-Requested-With, X-Authorization",
            "X-Content-Type-Options" : "nosniff",
            "x-frame-options" : "DENY",
            "x-xss-protection" : "1; mode=block"
        },		
		"bodyFileName": "COSME0004GenerateOTPResponse.json"
	}
}
